{
  "name": "react-starter-project",
  "version": "0.0.0",
  "description": "Starter React project that uses webpack, react-router, and redux.",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --config webpack.config.dev.babel.js",
    "build:prod:server": "NODE_ENV=production babel --extensions .ts src/server --out-dir build/dist --ignore src/server/**/*.d.ts",
    "build:prod:client": "NODE_ENV=production webpack --progress --colors --config webpack.config.prod.babel.js",
    "build:prod": "npm run build:prod:client && npm run build:prod:server",
    "postinstall": "npm run build:prod",
    "start": "node build/dist/server.js",
    "test": "jest --config jest-config.json && npm run lint",
    "lint:eslint": "eslint src test --ext .js,.jsx,.ts,.tsx",
    "lint:stylelint": "stylelint src/**/*.scss",
    "lint": "npm run lint:eslint; npm run lint:stylelint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djtsai/react-starter-project.git"
  },
  "engines": {
    "node": "^14.4.0",
    "npm": "^6.14.5"
  },
  "author": "Daniel Tsai <danieltsai.developer@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.6",
    "helmet-csp": "^2.10.0",
    "is-port-reachable": "^3.0.0",
    "morgan": "^1.10.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.0.5",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/morgan": "^1.9.0",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-logger": "^3.0.8",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.0.0",
    "terser-webpack-plugin": "^3.0.3",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
